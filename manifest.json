{
  "name": "Job Bookmark Extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Bookmarking Jobs With Booky.",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
      "bookmarks",
      "scripting",
      "activeTab"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html"
  },
  "host_permissions": [
      "<all_urls>"
  ]
}
